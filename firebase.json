{
  "hosting": {
    "public": "docs/.vuepress/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/installation.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-ago=0, no-cache"
          }
        ]
      }
    ]
  }
}
